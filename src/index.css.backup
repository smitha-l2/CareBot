/* Responsive Design for Follow-up Scheduler */
@media (max-width: 768px) {
  .scheduler-header {
    flex-direction: column;
    align-items: stretch;
    text-align: center;
  }
  
  .back-btn {
    order: -1;
    margin-bottom: 15px;
    align-self: flex-start;
    min-width: 200px;
  }
  
  .stats-grid {
    grid-template-columns: repeat(2, 1fr);
  }
  
  .form-grid {
    grid-template-columns: 1fr;
  }
  
  .form-actions {
    flex-direction: column;
  }
  
  .schedule-btn,
  .send-now-btn {
    min-width: unset;
  }
}

@media (max-width: 480px) {
  .stats-grid {
    grid-template-columns: 1fr;
  }
  
  .follow-up-scheduler {
    padding: 15px;
    margin: 15px 0;
  }
}

/* Enhanced Follow-up Scheduler Styles with Beautiful Backgrounds */


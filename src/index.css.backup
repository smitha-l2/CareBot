  font-weight: 700;
  margin-bottom: 12px;
  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.3);
}

.visit-type-info p {
  color: rgba(255, 255, 255, 0.9);
  margin-bottom: 8px;
  font-weight: 500;
  line-height: 1.5;
}

.custom-timing {
  color: #4caf50 !important;
  font-weight: 600 !important;
  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2) !important;
}

/* Follow-up Message Result */
.followup-message-result {
  background: linear-gradient(145deg, rgba(76, 175, 80, 0.2), rgba(76, 175, 80, 0.1));
  backdrop-filter: blur(15px);
  border-radius: 20px;
  padding: 25px;
  margin-top: 25px;
  border: 1px solid rgba(76, 175, 80, 0.3);
  box-shadow: 0 8px 32px rgba(76, 175, 80, 0.2);
}

.followup-message-result h3 {
  color: white;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);
  margin-bottom: 20px;
  font-size: 18px;
  font-weight: 700;
}

.message-info {
  background: rgba(255, 255, 255, 0.9);
  backdrop-filter: blur(10px);
  border-radius: 12px;
  padding: 18px;
  margin-bottom: 20px;
  border: 1px solid rgba(255, 255, 255, 0.3);
}

.message-info p {
  color: #2c3e50;
  margin-bottom: 8px;
  font-weight: 500;
}

.whatsapp-url-container {
  background: rgba(255, 255, 255, 0.9);
  backdrop-filter: blur(10px);
  border-radius: 12px;
  padding: 18px;
  border: 1px solid rgba(255, 255, 255, 0.3);
}

.whatsapp-url-container p {
  color: #2c3e50;
  font-weight: 600;
  margin-bottom: 12px;
}

.whatsapp-url-link {
  display: inline-block;
  background: linear-gradient(45deg, #25d366, #20c653);
  color: white;
  padding: 12px 24px;
  border-radius: 25px;
  text-decoration: none;
  font-weight: 700;
  transition: all 0.3s ease;
  box-shadow: 0 6px 20px rgba(37, 211, 102, 0.4);
  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2);
}

.whatsapp-url-link:hover {
  background: linear-gradient(45deg, #20c653, #25d366);
  transform: translateY(-2px);
  box-shadow: 0 8px 25px rgba(37, 211, 102, 0.5);
  text-decoration: none;
  color: white;
}

/* Animated Background Elements */
.follow-up-scheduler::after {
  content: '✨';
  position: absolute;
  top: 20px;
  right: 20px;
  width: 100px;
  height: 100px;
  font-size: 50px;
  opacity: 0.15;
  animation: float 6s ease-in-out infinite;
  display: flex;
  align-items: center;
  justify-content: center;
}

@keyframes float {
  0%, 100% { transform: translateY(0px) rotate(0deg); }
  50% { transform: translateY(-20px) rotate(180deg); }
}

/* Additional Floating Elements */
.scheduler-header::after {
  content: 'ðŸ©º';
  position: absolute;
  top: 15px;
  right: 15px;
  font-size: 20px;
  animation: twinkle 2s ease-in-out infinite;
}

@keyframes twinkle {
  0%, 100% { opacity: 0.3; transform: scale(1); }
  50% { opacity: 1; transform: scale(1.2); }
}

.patient-summary-section::after {
  content: 'ðŸ‘¥';
  position: absolute;
  top: 10px;
  right: 15px;
  font-size: 24px;
  opacity: 0.3;
  animation: bounce 3s ease-in-out infinite;
}

@keyframes bounce {
  0%, 100% { transform: translateY(0px); }
  50% { transform: translateY(-10px); }
}

@media (max-width: 768px) {
  .follow-up-scheduler {
    padding: 20px;
    border-radius: 15px;
  }
  
  .patient-summary-grid {
    grid-template-columns: 1fr;
  }
  
  .form-grid {
    grid-template-columns: 1fr;
  }
  
  .followups-header {
    flex-direction: column;
    align-items: stretch;
  }
  
  .followups-controls {
    justify-content: space-between;
  }
  
  .summary-stats {
    flex-direction: column;
    gap: 20px;
  }
  
  .quick-schedule-buttons {
    justify-content: center;
  }
  
  .form-actions {
    flex-direction: column;
    align-items: stretch;
  }
  
  .primary-btn, .secondary-btn {
    min-width: auto;
  }
  
  .followups-grid {
    grid-template-columns: 1fr;
  }
}

@media (max-width: 480px) {
  .follow-up-scheduler {
    margin: 15px 0;
    padding: 15px;
  }
  
  .patient-summary-section,
  .schedule-followup-form,
  .scheduled-followups-section {
    padding: 20px;
  }
  
  .patient-summary-card {
    min-height: 80px;
    padding: 15px;
  }
  
  .followups-summary {
    padding: 20px;
  }
  
  .stat-number {
    font-size: 24px;
  }
  
  .followup-card.enhanced {
    padding: 20px;
  }
  
  .scheduler-header,
  .follow-up-stats,
  .visit-type-info,
  .form-validation-feedback,
  .followup-message-result {
    padding: 15px;
  }
}


/* Filter Section */
.followups-filter {
  display: flex;
  align-items: center;
  gap: 15px;
  margin-bottom: 25px;
  padding: 20px;
  background: linear-gradient(135deg, rgba(255, 255, 255, 0.95), rgba(255, 255, 255, 0.85));
  backdrop-filter: blur(15px);
  -webkit-backdrop-filter: blur(15px);
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 20px;
  box-shadow: 
    0 8px 32px rgba(0, 0, 0, 0.1),
    0 0 0 1px rgba(255, 255, 255, 0.2);
}

.filter-select {
  background: linear-gradient(135deg, rgba(255, 255, 255, 0.9), rgba(255, 255, 255, 0.7));
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
  border: 2px solid rgba(255, 255, 255, 0.3);
  border-radius: 15px;
  padding: 12px 20px;
  font-size: 16px;
  color: #333;
  outline: none;
  transition: all 0.3s ease;
  cursor: pointer;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.05);
  min-width: 150px;
}

.filter-select:hover {
  border-color: rgba(102, 126, 234, 0.5);
  box-shadow: 0 8px 30px rgba(102, 126, 234, 0.2);
  transform: translateY(-2px);
}

.filter-select:focus {
  border-color: rgba(102, 126, 234, 0.7);
  box-shadow: 
    0 8px 30px rgba(102, 126, 234, 0.3),
    0 0 0 3px rgba(102, 126, 234, 0.1);
}

.filter-select option {
  background: rgba(255, 255, 255, 0.95);
  color: #333;
  padding: 10px;
}


